package kr.hs.study.beans;

import org.springframework.stereotype.Component;

@Component
public class HyunDai implements Car{
	String name;
	String color;
	
	public HyunDai() {
		
	}
	public String getName() {
		return name;
	}

	public String getColor() {
		return color;
	}

	public int money(int money) {
		return money;
	}

	public void Order() {
		
	}

}
